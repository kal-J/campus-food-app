{
  "name": "lutrade",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "cod-scripts test",
    "lint": "eslint .",
    "format": "cod-scripts format",
    "lint:commit": "cod-scripts commitlint"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-regenerator",
      "@babel/plugin-transform-runtime"
    ],
    "sourceType": "unambiguous"
  },
  "husky": {
    "hooks": {
      "pre-commit": "cod-scripts pre-commit",
      "commit-msg": "cod-scripts commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@react-native-community/netinfo": "^5.8.0",
    "expo-font": "^8.1.1",
    "jwt-decode": "^2.2.0",
    "native-base": "git+https://git@github.com/kal-J/NativeBase.git",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-auth0": "^2.3.0",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-image-picker": "^0.28.1",
    "react-native-reanimated": "^1.8.0",
    "react-native-screens": "^2.5.0",
    "react-native-unimodules": "^0.9.0",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^4.3.8",
    "react-navigation-drawer": "^2.4.12",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-transform-regenerator": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/runtime": "7.9.2",
    "@bam.tech/react-native-make": "^3.0.0",
    "@react-native-community/eslint-config": "0.0.5",
    "babel-jest": "24.9.0",
    "cod-scripts": "^4.0.3",
    "eslint": "6.8.0",
    "jest": "24.9.0",
    "jest-expo": "^37.0.0",
    "metro-react-native-babel-preset": "0.58.0",
    "react-test-renderer": "16.11.0"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(css|less)$": "<rootDir>/mocks/fileMock.js"
    }
  }
}
